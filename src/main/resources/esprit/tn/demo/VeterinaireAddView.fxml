<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="10" alignment="CENTER" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="esprit.tn.demo.controllers.GestionAnimal.VeterinaireAddController">

    <TextField fx:id="nomField" promptText="Nom"/>
    <TextField fx:id="numTelField" promptText="Numéro de téléphone"/>
    <TextField fx:id="emailField" promptText="Email"/>
    <TextField fx:id="adresseCabineField" promptText="Adresse du cabinet"/>
    <Button text="Ajouter" onAction="#handleAddVeterinaire"/>
</VBox>
